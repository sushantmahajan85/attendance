<!DOCTYPE html>
<html>
  <head>
    <title>Attendance Manager</title>
    
  </head>
  <body>
      <h1>Edit Page</h1>
      
      <% attendance.forEach(function(meeting){ %>
        <br>
        <h3 style="display: inline;">Date</h3>   : <%= meeting.Date.toDateString().split(' ').slice(0,3).join(' ');   %>
        <h3>EDIT Attendance</h3>
        Payas       : <%= meeting.Payas %>      <button onclick="edit(<%=meeting.Payas%>)">Edit</button>
        <br>
        Sushant     : <%= meeting.Sushant %>    <button onclick="edit(<%=meeting.Sushant%>)">Edit</button>
        <br>
        Saksham     : <%= meeting.Saksham %>    <button onclick="edit(<%=meeting.Saksham%>)">Edit</button>
        <br>
      <% }); %>
      <script type="text/javascript" src="../edit.js"></script>
  </body>
</html>
