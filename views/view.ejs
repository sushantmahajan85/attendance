<!DOCTYPE html>
<html>
  <head>
    <title>Attendance Manager</title>
  </head>
  <body>

      <h1>View Page</h1>
      <% attendance.forEach(function(meeting){ %>
        <br>
        <h3 style="display: inline;">Date</h3>   : <%= meeting.Date.toDateString().split(' ').slice(0,3).join(' ');   %>
        <h3>Attendance</h3>
        Payas       : <%= meeting.Payas %>
        <br>
        Sushant     : <%= meeting.Sushant %>
        <br>
        Saksham     : <%= meeting.Saksham %>
        <br>
      <% }); %>
  </body>
</html>
